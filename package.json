{
  "name": "sd.cpp-studio",
  "version": "1.0.0",
  "description": "Web interface for Stable Diffusion via stable-diffusion.cpp with OpenAI-compatible API",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && node --watch server.js",
    "dev:frontend": "cd frontend && vite",
    "build": "cd frontend && vite build",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "models": "node backend/scripts/models-cli.js",
    "models:list": "node backend/scripts/models-cli.js list",
    "models:info": "node backend/scripts/models-cli.js info",
    "models:files": "node backend/scripts/models-cli.js files",
    "models:running": "node backend/scripts/models-cli.js running",
    "test:logging": "node scripts/test-sdcpp-logging.js"
  },
  "keywords": [
    "stable-diffusion",
    "openai",
    "image-generation"
  ],
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "formdata-node": "^6.0.3",
    "happy-dom": "^20.0.11",
    "jsdom": "^27.3.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  },
  "workspaces": [
    "backend",
    "frontend"
  ],
  "dependencies": {
    "js-yaml": "^4.1.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "ws": "^8.18.3"
  }
}
